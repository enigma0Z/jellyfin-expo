    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "24.7.0",
    "eslint-plugin-promise": "5.2.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-native": "3.11.0",
    "jest": "28.1.3",
    "jest-expo": "51.0.4",
